#!/usr/bin/env bash

### VSCode extensions
if command -v code > /dev/null 2>&1; then
  extensions=(
    'ms-vscode.theme-tomorrowkit'
    'pustelto.bracketeer'
    'humao.rest-client'
    'mhutchie.git-graph'
    'dbaeumer.vscode-eslint'
    'mikestead.dotenv'
  )
  for extension in ${extensions[@]}; do
    code --install-extension $extension;
  done
fi
###